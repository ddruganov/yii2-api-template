{
  "name": "ddruganov/yii2-api-template",
  "description": "Yii 2 Api Project Template",
  "keywords": [
    "yii2",
    "api",
    "project template"
  ],
  "type": "project",
  "minimum-stability": "stable",
  "require": {
    "php": "^8",
    "yiisoft/yii2": "^2",
    "ddruganov/yii2-api-auth": "^1"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
